# Frontend Environment Variables - EXAMPLE FILE
# Copy this to .env and update with your actual values

# API Configuration
VITE_API_BASE_URL=http://localhost:8080/api

# Environment
VITE_NODE_ENV=development

# JWT Configuration (keys for localStorage)
VITE_JWT_ACCESS_TOKEN_KEY=access_token
VITE_JWT_REFRESH_TOKEN_KEY=refresh_token

# Request timeout (in milliseconds)
VITE_API_TIMEOUT=10000